#: src/rsistats.cpp:54
#, kde-format
msgid "Number of postponed short breaks (user)"
msgstr "स्थगितानां लघुविरामानाम् संख्या (उपयोक्ता) ."

#: src/rsistats.cpp:57
#, kde-format
msgid "Last short break"
msgstr "अन्तिमः लघुविरामः"

#: src/rsistats.cpp:59
#, kde-format
msgid "Total number of long breaks"
msgstr "दीर्घविरामानाम् कुलसंख्या"

#: src/rsistats.cpp:63
#, kde-format
